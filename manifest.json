{
  "manifest_version": 3,
  "name": "minimal-startpage-leo",
  "short_name": "minimal-startpage",
  "version": "1.2.0",
  "description": "Overrides the browsers newtab page with a custom startpage.",
  "author": "Leo",
  "icons": {
    "64": "img/icon64.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "minimal-startpage@craftcat.dev"
    }
  }
}
